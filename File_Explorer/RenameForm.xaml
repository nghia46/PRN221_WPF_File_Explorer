<Window x:Class="File_Explorer.RenameForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:File_Explorer"
        mc:Ignorable="d"
        Title="RenameForm" Height="177" Width="530">
    <Window.Icon>
        <BitmapImage UriSource="D:\Repos\PRN221\File_Explorer\File_Explorer\Resources\folder.png"/>
    </Window.Icon>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="737*"/>
            <ColumnDefinition Width="63*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="SubmitBtn" Content="Submit" HorizontalAlignment="Right" Margin="0,0,0,0" VerticalAlignment="Center" FontSize="20" Width="100" Click="SubmitBtn_Click"/>
        <TextBox Grid.Column="0" x:Name="RenameTxt" HorizontalAlignment="Left" Margin="53,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="320" Height="30" FontSize="18"/>

    </Grid>
</Window>
